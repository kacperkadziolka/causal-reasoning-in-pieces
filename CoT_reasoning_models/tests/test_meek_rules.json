    "expected": [["B", "E"], ["C", "E"], ["D", "E"], ["E", "F"]]
  },
  {
    "name": "M-structure",
    "description": "Common causal discovery example",
    "skeleton": [["A", "B"], ["B", "C"], ["C", "D"], ["D", "E"]],
    "v_structures": [["A", "B", "C"], ["C", "D", "E"]],
    "expected": [["A", "B"], ["C", "B"], ["C", "D"], ["E", "D"]]
  },
  {
    "name": "Larger DAG",
    "description": "More complex case with several rules",
    "skeleton": [["A", "B"], ["B", "C"], ["A", "D"], ["B", "E"],
