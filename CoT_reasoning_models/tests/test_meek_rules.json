  {
    "name": "Graph with two v-structures",
    "description": "Tests orientation with multiple v-structures",
    "skeleton": [["A", "E"], ["B", "C"], ["D", "E"], ["B", "D"], ["C", "D"], ["A", "C"]],
    "v_structures": [["B", "C", "A"], ["D", "E", "A"]],
