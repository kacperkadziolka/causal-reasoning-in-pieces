    "skeleton": [["A", "B"], ["B", "C"], ["B", "D"], ["B", "E"]],
    "v_structures": [["C", "B", "D"], ["C", "B", "E"]],
    "expected": [["C", "B"], ["D", "B"], ["E", "B"], ["B", "A"]]
  },
  {
    "name": "Triangle with tail",
