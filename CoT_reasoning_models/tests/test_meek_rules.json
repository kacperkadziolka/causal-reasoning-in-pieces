    "skeleton": [["A", "B"], ["B", "C"], ["A", "C"], ["C", "D"]],
    "v_structures": [["A", "C", "D"]],
    "expected": [["A", "C"], ["D", "C"], ["A", "B"], ["C", "B"]]
